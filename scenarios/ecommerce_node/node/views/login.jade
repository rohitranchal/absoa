doctype html
html
	include header
	script(src="./javascripts/shop.js")
	body
		include nav_bar_without_checkout
		.container
			.main-content(id="login_content")
				block content
					form(class="form-horizontal" role="form" action="login-complete" method="post")
						div(class="form-group")
							label(for="inputEmail3" class="col-sm-2 control-label") Email
							div(class="col-sm-10")
								input(type="email" class="form-control" id="inputEmail3" placeholder="Email" name="email")
						div(class="form-group")
							label(for="inputPassword3" class="col-sm-2 control-label") Password
							div(class="col-sm-10")
								input(type="password" class="form-control" id="inputPassword3" placeholder="Password" name="password")
						div(class="form-group")
							div(class="col-sm-offset-2 col-sm-10")
								button(type="submit" class="btn btn-default") Sign in
	footer
		p  ABSOA E-Commerce Scenario (Running on node with Express, Jade and Stylus)
